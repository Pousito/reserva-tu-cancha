{
  "name": "reserva-tu-cancha",
  "version": "1.0.0",
  "description": "Sistema de reservas de canchas deportivas",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "deploy": "node auto-deploy.js",
    "quick-deploy": "git add . && git commit -m \"Quick update\" && git push origin main",
    "deploy-full": "node deploy_and_populate.js deploy",
    "populate-db": "node deploy_and_populate.js populate",
    "check-db": "node deploy_and_populate.js check",
    "check-render": "node check_render_status.js",
    "populate-reservas": "node populate_reservas.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.4",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "reservas",
    "canchas",
    "deportes",
    "padel",
    "futbol"
  ],
  "author": "Tu Nombre",
  "license": "MIT"
}
